<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jinkworld.downloadmanager">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver
            android:name=".DownloadReceiver"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".aige.lesson1.Lesson1Activity" />
        <activity android:name=".pie.PieActivity" />
        <activity android:name=".lesson5.Lesson5Activity" />
        <activity android:name=".lesson6.Lesson6Activity" />
        <activity android:name=".lesson7.Lesson7Activity" />
        <activity android:name=".commandpattern.DrawActivity" />
        <activity android:name=".memotopattern.MemotoActivity"></activity>
    </application>

</manifest>